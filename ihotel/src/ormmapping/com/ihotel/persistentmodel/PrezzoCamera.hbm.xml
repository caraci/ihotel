<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ihotel.persistentmodel.PrezzoCamera" table="PrezzoCamera" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="_periodo" cascade="lock" column="PeriodoID" class="com.ihotel.persistentmodel.Periodo" not-null="false" lazy="no-proxy">
		</many-to-one>
		<many-to-one name="_descrizioneCamera" cascade="lock" column="DescrizioneCameraID" class="com.ihotel.persistentmodel.DescrizioneCamera" not-null="false" lazy="no-proxy">
		</many-to-one>
		<property name="_prezzo" column="_prezzo" type="integer" length="11" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
