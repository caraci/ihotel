<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ihotel.persistentmodel.DescrizioneCamera" table="DescrizioneCamera" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="_catalogoCamere" cascade="lock" column="CatalogoCamereID" class="com.ihotel.persistentmodel.CatalogoCamere" not-null="false" lazy="no-proxy">
		</many-to-one>
		<set name="_prezziCamera" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="DescrizioneCameraID" not-null="false"/>
			<one-to-many class="com.ihotel.persistentmodel.PrezzoCamera"/>
		</set>
	</class>
</hibernate-mapping>
