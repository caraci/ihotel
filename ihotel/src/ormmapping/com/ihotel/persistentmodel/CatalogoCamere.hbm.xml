<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ihotel.persistentmodel.CatalogoCamere" table="CatalogoCamere" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<set name="_descrizioniCamere" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="CatalogoCamereID" not-null="false"/>
			<one-to-many class="com.ihotel.persistentmodel.DescrizioneCamera"/>
		</set>
	</class>
</hibernate-mapping>
