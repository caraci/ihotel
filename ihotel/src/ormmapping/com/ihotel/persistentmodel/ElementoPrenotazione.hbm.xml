<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ihotel.persistentmodel.ElementoPrenotazione" table="ElementoPrenotazione" lazy="false">
		<meta attribute="class-description">
			@author Eugenio
		</meta>
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="_prenotazione" cascade="lock" column="PrenotazioneID" class="com.ihotel.persistentmodel.Prenotazione" not-null="false" lazy="no-proxy">
		</many-to-one>
		<many-to-one name="_camera" cascade="lock" column="CameraID" class="com.ihotel.persistentmodel.Camera" not-null="false" lazy="no-proxy">
		</many-to-one>
	</class>
</hibernate-mapping>
