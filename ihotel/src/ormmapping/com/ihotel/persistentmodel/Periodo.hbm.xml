<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ihotel.persistentmodel.Periodo" table="Periodo" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="_dataInizio" column="_dataInizio" type="date" not-null="true" lazy="false"/>
		<property name="_dataFine" column="_dataFine" type="date" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
