<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ihotel.persistentmodel.Camera" table="Camera" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="_albergo" cascade="lock" column="AlbergoID" class="com.ihotel.persistentmodel.Albergo" not-null="false" lazy="no-proxy">
		</many-to-one>
		<property name="_numero" column="_numero" type="string" length="255" not-null="false" lazy="false"/>
		<property name="_tipologia" column="_tipologia" type="string" length="255" not-null="false" lazy="false"/>
		<set name="_statiCamera" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="CameraID" not-null="false"/>
			<one-to-many class="com.ihotel.persistentmodel.StatoCamera"/>
		</set>
	</class>
</hibernate-mapping>
