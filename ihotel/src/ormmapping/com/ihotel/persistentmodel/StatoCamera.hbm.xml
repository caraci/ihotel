<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ihotel.persistentmodel.StatoCamera" table="StatoCamera" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="_periodo" cascade="lock" column="PeriodoID" class="com.ihotel.persistentmodel.Periodo" not-null="false" lazy="no-proxy">
		</many-to-one>
		<many-to-one name="_camera" cascade="lock" column="CameraID" class="com.ihotel.persistentmodel.Camera" not-null="false" lazy="no-proxy">
		</many-to-one>
		<property name="_libera" column="_libera" type="boolean" length="1" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
